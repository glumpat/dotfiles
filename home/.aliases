# Files
alias aliases="vim ~/.aliases"
alias zshrc="vim ~/.zshrc"
alias vimrc="vim ~/.vimrc"
alias config="vim ~/.vim/config.vim"
alias shared="vim ~/.vim/shared.vim"
alias ideavimrc="vim ~/.ideavimrc"
alias todo="vim ~/Documents/Todo.md"
alias notes="vim ~/Documents/Notes.md"

# Command aliases
alias t="gnome-terminal"

# Git
alias gmm="git merge master"
alias gaacm="git add -A && git commit -m"

# Zapier
alias zd="zapier describe"
alias ze="zapier env"
alias zeg="zapier env:get"
alias zes="zapier env:set"
alias zhist="zapier history"
alias zp="zapier push"
alias zpro="zapier promote"
alias zt="zapier test"
alias zua="zapier users:add"
alias zul="zapier users:links"
alias zup="zapier upload"
alias zur="zapier users:links"
alias zv="zapier validates"
alias zver="zapier versions"


# Directories
alias dl="cd ~/Downloads"
alias src="cd ~/Source"

# Close terminal window
alias :q='exit'

# Directories
alias -g ...='../..'
alias -g ....='../../..'
alias -- -='cd -'
alias lsa='ls -lah'
alias l='ls -lah'
alias ll='ls -lh'
alias la='ls -lAh'
