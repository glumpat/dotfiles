# Files
alias zshrc="vim ~/.zshrc"
alias vimrc="vim ~/.vimrc"
alias config="vim ~/.vim/config.vim"
alias shared="vim ~/.vim/shared.vim"
alias plugins="vim ~/.vim/plugins.vim"
alias ideavimrc="vim ~/.ideavimrc"
alias todo="vim ~/Documents/Todo.md"
alias notes="vim ~/Documents/Notes.md"

# Command aliases
alias t="gnome-terminal"
alias gaacm="git add -A && git commit -m"
alias wiki="tiddlywiki ~/.wiki --server 8099"

# Directories
alias dl="cd ~/Downloads"

# Clean terminal window
alias :q='exit'

# Use bat
alias cat='bat'

# Directories
alias -g ...='../..'
alias -g ....='../../..'
alias -- -='cd -'
alias lsa='ls -lah'
alias l='ls -lah'
alias ll='ls -lh'
alias la='ls -lAh'
