# Files
alias aliases="vim ~/.aliases"
alias zshrc="vim ~/.zshrc"
alias vimrc="vim ~/.vimrc"
alias config="vim ~/.vim/config.vim"
alias shared="vim ~/.vim/shared.vim"
alias ideavimrc="vim ~/.ideavimrc"
alias todo="vim ~/Documents/Todo.md"
alias notes="vim ~/Documents/Notes.md"

# Command aliases
alias t="gnome-terminal"

# Git
alias gmm="git merge master"
alias gaacm="git add -A && git commit -m"

# Directories
alias dl="cd ~/Downloads"
alias src="cd ~/Source"

# Close terminal window
alias :q='exit'

# Directories
alias -g ...='../..'
alias -g ....='../../..'
alias -- -='cd -'
alias lsa='ls -lah'
alias l='ls -lah'
alias ll='ls -lh'
alias la='ls -lAh'
